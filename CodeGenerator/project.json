    "compilationOptions": {
        "define": [ "DNX" ]
    },

    "configurations": {
        "Debug": {
            "compilationOptions": {
                "define": [ "DEBUG", "DNX" ]
            }
        },
        "Release": {
            "compilationOptions": {
                "define": [ "RELEASE", "DNX" ],
                "optimize": true
            }
        }
    },

    "commands": {
        "run": "SilentOrbit.ProtocolBuffers.Program"
    },

    "dependencies": { "CodeWriter": "1.0-*" },

    "resource": "ProtocolParser\\ProtocolParser*.cs",

    "frameworks": {
        "dnx451": {
            "compilationOptions": {
                "define": [ "RELEASE", "DNX" ],
                "optimize": true
            }
        },
        "dnxcore50": {
            "compilationOptions": {
                "define": [ "RELEASE", "DNX" ],
                "optimize": true
            },
            "dependencies": {
                "System.Collections": "4.0.0.0",
                "System.Collections.Concurrent": "4.0.0.0",
                "System.ComponentModel": "4.0.0.0",
                "System.Linq": "4.0.0.0",
                "System.Reflection": "4.0.10.0",
                "System.Runtime": "4.0.20.0",
                "System.Runtime.InteropServices": "4.0.10.0",
                "System.Threading": "4.0.0.0",
                "System.Threading.Tasks": "4.0.0.0",
                "System.Console": "4.0.0-*",
                "System.IO.FileSystem": "4.0.0"
            }
        }
    }
}