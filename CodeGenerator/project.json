    "compilationOptions": {
        "emitEntryPoint": true,
        "define": [ "DNX" ]
    },

    "configurations": {
        "Debug": {
            "compilationOptions": {
                "define": [ "DEBUG" ]
            }
        },
        "Release": {
            "compilationOptions": {
                "define": [ "RELEASE" ],
                "optimize": true
            }
        }
    },

    "dependencies": {
        "CodeWriter": "1.0-*"
    },

    "commands": {
        "run": "CodeGenerator"
    },

    "resource": "ProtocolParser\\ProtocolParser*.cs",

    "frameworks": {
        "dnx451": { },
        "dnxcore50": {
            "dependencies": {
                "Microsoft.CSharp": "4.0.1-*",
                "System.Collections": "4.0.11-*",
                "System.Console": "4.0.0-*",
                "System.Linq": "4.0.1-*",
                "System.Threading": "4.0.11-*"
            }
        }
    }
}